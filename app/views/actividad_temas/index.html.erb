<p id="notice"><%= notice %></p>

<h1>Actividad Temas</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actividad_temas.each do |actividad_tema| %>
      <tr>
        <td><%= actividad_tema.descripcion %></td>
        <td><%= link_to 'Show', actividad_tema %></td>
        <td><%= link_to 'Edit', edit_actividad_tema_path(actividad_tema) %></td>
        <td><%= link_to 'Destroy', actividad_tema, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actividad Tema', new_actividad_tema_path %>
